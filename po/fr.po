msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr ""
"-tmpdir <repertoiretemporaire> : utiliser un autre répertoire de travail "
"(ex : /var/ramdisk)\n"

#: src/main.c:2438
msgid "-set <setname>   : autoload clip set setname\n"
msgstr ""
"-set <nom_du_set> : charger automatiquement l'ensemble de clips nom_du_set\n"

