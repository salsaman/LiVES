#: src/main.c:3346
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [OPTS] [filename [start_time] [frames]]\n"
msgstr ""

#: src/main.c:3347
msgid "Where: filename is the name of a media file or backup file to import\n"
msgstr ""

#: src/main.c:3348
msgid "start_time : filename start time in seconds\n"
msgstr "start_time: tempo di avvio di filename in secondi\n"

#: src/main.c:3349
msgid "frames : maximum number of frames to open\n"
msgstr "fotogrammi: numero massimo di fotogrammi da aprire\n"

#: src/main.c:3351
msgid "OPTS can be:\n"
msgstr "Le opzioni possono essere:\n"

#: src/main.c:3352
msgid "-help | --help \t\t\t: print this help text on stderr and exit\n"
msgstr ""

#: src/main.c:3353
msgid "-version | --version\t\t: print the LiVES version on stderr and exit\n"
msgstr ""

#: src/main.c:3354
msgid ""
"-workdir <workdir>\t\t: specify the working directory for the session, "
"overriding any value set in preferences\n"
msgstr ""

#: src/main.c:3356
msgid "\t\t\t\t\t(disables any disk quota checking)"
msgstr "\t\t\t\t\t(disabilita qualsiasi controllo della quota del disco)"

#: src/main.c:3357
msgid ""
"-configfile <path_to_file>\t: override the default configuration file for "
"the session\n"
msgstr ""

#: src/main.c:3359
#, c-format
msgid "\t\t\t\t\t(default is %s)\n"
msgstr "\t\t\t\t\t(il predefinito è %s)\n"

#: src/main.c:3363
msgid ""
"-configdatadir <dir>\t\t: override the default configuration data directory "
"for the session\n"
msgstr ""

#: src/main.c:3364
#, c-format
msgid "\t\t\t\t\t(default is %s\n"
msgstr "\t\t\t\t\t(il predefinito è %s\n"

#: src/main.c:3367
msgid ""
"-dscrit <bytes>\t\t\t: temporarily sets the free disk space critical level "
"for workdir to <bytes>\n"
msgstr ""

#: src/main.c:3368
msgid ""
"\t\t\t\t\t(intended to allow correction of erroneous values within the app; "
"<= 0 disables checks)\n"
msgstr ""

#: src/main.c:3370
msgid "-set <setname>\t\t\t: autoload clip set <setname>\n"
msgstr ""

#: src/main.c:3371
msgid ""
"-noset\t\t\t\t: do not reload any clip set on startup (overrides -set)\n"
msgstr ""

#: src/main.c:3372
msgid ""
"-layout <layout_name>\t\t: autoload multitrack layout <layout_name> (if "
"successful, overrides -startup-ce)\n"
msgstr ""

#: src/main.c:3374
msgid ""
"-nolayout\t\t\t: do not reload any multitrack layout on startup (overrides -"
"layout)\n"
msgstr ""

#: src/main.c:3375
msgid ""
"-norecover\t\t\t: force non-loading of crash recovery files (overrides -"
"recover / -autorecover)\n"
msgstr ""

#: src/main.c:3377
msgid ""
"-recover | -autorecover\t\t: force reloading of any crash recovery files "
"(may override -noset and -nolayout)\n"
msgstr ""

#: src/main.c:3378
msgid ""
"-nogui\t\t\t\t: do not show the gui (still shows the play window when "
"active)\n"
msgstr ""

#: src/main.c:3379
msgid "-nosplash\t\t\t: do not show the splash window\n"
msgstr ""

#: src/main.c:3381
msgid ""
"-noplaywin\t\t\t: do not show the play window (still shows the internal "
"player; intended for remote streaming)\n"
msgstr ""

#: src/main.c:3383
msgid ""
"-noninteractive\t\t\t: disable menu interactivity (intended for scripting "
"applications, e.g liblives)\n"
msgstr ""

#: src/main.c:3384
msgid "-startup-ce\t\t\t: start in clip editor mode (overrides -startup-mt)\n"
msgstr ""

#: src/main.c:3385
msgid "-startup-mt\t\t\t: start in multitrack mode\n"
msgstr "-startup-mt\t\t\t: avvia in modalità multi-traccia\n"

#: src/main.c:3386
msgid ""
"-vjmode\t\t\t\t: start in VJ mode (implicitly sets -startup-ce -autorecover -"
"nolayout -asource external)\n"
msgstr ""

#: src/main.c:3389
msgid ""
"-fxmodesmax <n>\t\t\t: allow <n> modes per effect key (overrides any value "
"set in preferences; minimum is 1)\n"
msgstr ""

#: src/main.c:3391
#, c-format
msgid ""
"-oscstart <port>\t\t: start OSC listener on UDP port <port> (default is %d)\n"
msgstr ""

#: src/main.c:3393
msgid ""
"-nooscstart\t\t\t: do not start the OSC listener (the default, unless set in "
"preferences)\n"
msgstr ""

#: src/main.c:3396
msgid ""
"-asource <source>\t\t: set the initial audio source (<source> can be "
"'internal' or 'external')\n"
msgstr ""

#: src/main.c:3397
#, c-format
msgid "\t\t\t\t\t(only valid for %s and %s players)\n"
msgstr "\t\t\t\t\t(valido solamente per i lettori %s e %s)\n"

#: src/main.c:3398
msgid ""
"-aplayer <ap>\t\t\t: start with the selected audio player (<ap> can be: "
msgstr ""

#: src/main.c:3410
msgid ""
"-jackopts <opts>\t\t: opts is a bitmap of jackd startup / playback options "
"(default is 16, unless set in Preferences)\n"
"\t\t\t\t\t 1 = LiVES is a jack transport slave, \n"
"\t\t\t\t\t 2 = LiVES is a jack transport master, \n"
"\t\t\t\t\t 4 = start/stop jack transport server on LiVES playback start / "
"stop\n"
"\t\t\t\t\t\t(must be transport master), \n"
"\t\t\t\t\t 8 = pause jack transport when video paused\n"
"\t\t\t\t\t\t(must be transport master),\n"
"\t\t\t\t\t16 = start/stop jack audio server on LiVES startup / shutdown\n"
"\t\t\t\t\t\t(only if audio player is jack)) \n"
msgstr ""

#: src/main.c:3425
#, c-format
msgid "'%s' or "
msgstr "'%s' oppure "

#: src/main.c:3428
msgid " or "
msgstr " oppure "

#: src/main.c:3432
msgid ""
"-devicemap <mapname>\t\t: autoload devicemap <mapname> (for MIDI / joystick "
"control)\n"
msgstr ""

#: src/main.c:3433
msgid ""
"-vppdefaults <file>\t\t: load defaults for video playback plugin from "
"<file>\n"
"\t\t\t\t\t(Note: only affects the plugin settings, not the plugin type)\n"
msgstr ""

