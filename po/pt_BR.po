#: src/main.c:3353
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [OPTS] [filename [start_time] [frames]]\n"
msgstr ""
"\n"
"A sintaxe de inicialização é: %s[OPTS] [nome_do_arquivo [tempo_inícial] "
"[quadros]]\n"

#: src/main.c:3354
msgid "Where: filename is the name of a media file or backup file to import\n"
msgstr ""
"Onde: nome_do_arquivo é o nome de um arquivo de mídia ou arquivo de backup "
"para importar\n"

#: src/main.c:3355
msgid "start_time : filename start time in seconds\n"
msgstr "tempo_inicial : tempo inicial do arquivo em segundos\n"

#: src/main.c:3356
msgid "frames : maximum number of frames to open\n"
msgstr "quadros : número máximo de quadros a abrir\n"

#: src/main.c:3358
msgid "OPTS can be:\n"
msgstr "OPTS pode ser:\n"

#: src/main.c:3359
msgid "-help | --help \t\t\t: print this help text on stderr and exit\n"
msgstr "-help | --help \t\t\t: imprime este texto de ajuda em stderr e sai\n"

#: src/main.c:3360
msgid "-version | --version\t\t: print the LiVES version on stderr and exit\n"
msgstr "-version | --version \t\t: imprime a versão do LiVES em stderr e sai\n"

#: src/main.c:3361
msgid ""
"-workdir <workdir>\t\t: specify the working directory for the session, "
"overriding any value set in preferences\n"
msgstr ""
"-workdir <workdir> \t\t: especifica o diretório de trabalho para a sessão, "
"substituindo qualquer valor definido nas preferências\n"

#: src/main.c:3363
msgid "\t\t\t\t\t(disables any disk quota checking)"
msgstr "\t\t\t\t\t(desativa qualquer verificação de cota de disco)"

#: src/main.c:3364
msgid ""
"-configfile <path_to_file>\t: override the default configuration file for "
"the session\n"
msgstr ""
"-configfile <caminho_para_arquivo> \t: sobrescreve o arquivo de configuração "
"padrão para a sessão\n"

#: src/main.c:3366
#, c-format
msgid "\t\t\t\t\t(default is %s)\n"
msgstr "\t\t\t\t\t(o padrão é %s)\n"

#: src/main.c:3370
msgid ""
"-configdatadir <dir>\t\t: override the default configuration data directory "
"for the session\n"
msgstr ""
"-configdatadir <dir> \t\t: substitui o diretório de dados de configuração "
"padrão para a sessão\n"

#: src/main.c:3371
#, c-format
msgid "\t\t\t\t\t(default is %s\n"
msgstr "\t\t\t\t\t(o padrão é %s\n"

#: src/main.c:3374
msgid ""
"-dscrit <bytes>\t\t\t: temporarily sets the free disk space critical level "
"for workdir to <bytes>\n"
msgstr ""
"-dscrit <bytes> \t\t\t: define temporariamente o nível crítico de espaço "
"livre em disco para o diretório de trabalho como <bytes>\n"

#: src/main.c:3375
msgid ""
"\t\t\t\t\t(intended to allow correction of erroneous values within the app; "
"<= 0 disables checks)\n"
msgstr ""
"\t\t\t\t\t(destina-se a permitir a correção de valores errados dentro do "
"aplicativo; <= 0 desativa as verificações)\n"

#: src/main.c:3377
msgid "-set <setname>\t\t\t: autoload clip set <setname>\n"
msgstr "-set <setname> \t\t\t: autocarregar clip set <setname>\n"

#: src/main.c:3378
msgid ""
"-noset\t\t\t\t: do not reload any clip set on startup (overrides -set)\n"
msgstr ""
"-noset \t\t\t\t: não recarrega nenhum conjunto de clipes na inicialização "
"(substitui -set)\n"

#: src/main.c:3379
msgid ""
"-layout <layout_name>\t\t: autoload multitrack layout <layout_name> (if "
"successful, overrides -startup-ce)\n"
msgstr ""
"-esboço  <esboço_nome> \t\t: auto carregamento do esboço da multipista "
"<esboço_nome> (se bem-sucedido, substitui -startup-ce)\n"

#: src/main.c:3381
msgid ""
"-nolayout\t\t\t: do not reload any multitrack layout on startup (overrides -"
"layout)\n"
msgstr ""
"-nolayout \t\t\t: não recarrega nenhum layout multipista na inicialização "
"(substitui -layout)\n"

#: src/main.c:3382
msgid ""
"-norecover\t\t\t: force non-loading of crash recovery files (overrides -"
"recover / -autorecover)\n"
msgstr ""
"-norecover \t\t\t: força o não carregamento dos arquivos de recuperação de "
"falha (substitui -recover / -autorecover)\n"

#: src/main.c:3384
msgid ""
"-recover | -autorecover\t\t: force reloading of any crash recovery files "
"(may override -noset and -nolayout)\n"
msgstr ""
"-recuperar | -autorecover \t\t: força o recarregamento de quaisquer arquivos "
"de recuperação de falha (pode substituir -noset e -nolayout)\n"

#: src/main.c:3385
msgid ""
"-nogui\t\t\t\t: do not show the gui (still shows the play window when "
"active)\n"
msgstr ""
"-nogui \t\t\t\t: não mostra o gui (ainda mostra a janela de reprodução "
"quando ativo)\n"

#: src/main.c:3386
msgid "-nosplash\t\t\t: do not show the splash window\n"
msgstr "-nosplash \t\t\t: não mostra a janela inicial\n"

#: src/main.c:3388
msgid ""
"-noplaywin\t\t\t: do not show the play window (still shows the internal "
"player; intended for remote streaming)\n"
msgstr ""
"-noplaywin \t\t\t: não mostra a janela de reprodução (ainda mostra o player "
"interno; destinado para streaming remoto)\n"

#: src/main.c:3390
msgid ""
"-noninteractive\t\t\t: disable menu interactivity (intended for scripting "
"applications, e.g liblives)\n"
msgstr ""
"-noninteractive \t\t\t: desativar a interatividade do menu (destinado a "
"aplicativos de script, por exemplo, liblives)\n"

#: src/main.c:3391
msgid "-startup-ce\t\t\t: start in clip editor mode (overrides -startup-mt)\n"
msgstr ""
"-startup-ce \t\t\t: inicia no modo de editor de clipe (substitui -startup-"
"mt)\n"

#: src/main.c:3392
msgid "-startup-mt\t\t\t: start in multitrack mode\n"
msgstr "-startup-mt \t\t\t: inicia no modo multipista\n"

#: src/main.c:3393
msgid ""
"-vjmode\t\t\t\t: start in VJ mode (implicitly sets -startup-ce -autorecover -"
"nolayout -asource external)\n"
msgstr ""
"-vjmode \t\t\t\t: iniciar no modo VJ (define implicitamente -startup-ce -"
"autorecover -nolayout -asource external)\n"

#: src/main.c:3396
msgid ""
"-fxmodesmax <n>\t\t\t: allow <n> modes per effect key (overrides any value "
"set in preferences; minimum is 1)\n"
msgstr ""
"-fxmodesmax <n>\t\t\t: permite <n> modos por chave de efeito (substitui "
"qualquer valor definido nas preferências; o mínimo é 1)\n"

#: src/main.c:3398
#, c-format
msgid ""
"-oscstart <port>\t\t: start OSC listener on UDP port <port> (default is %d)\n"
msgstr ""
"-oscstart <port> \t\t: inicia o monitor OSC na porta UDP <port> (o padrão é "
"%d)\n"

#: src/main.c:3400
msgid ""
"-nooscstart\t\t\t: do not start the OSC listener (the default, unless set in "
"preferences)\n"
msgstr ""
"-nooscstart \t\t\t: não inicia o monitor OSC (o padrão, a menos que definido "
"nas preferências)\n"

#: src/main.c:3403
msgid ""
"-asource <source>\t\t: set the initial audio source (<source> can be "
"'internal' or 'external')\n"
msgstr ""
"-asource <source> \t\t: define a fonte de áudio inicial (<source> pode ser "
"'internal' ou 'external')\n"

#: src/main.c:3404
#, c-format
