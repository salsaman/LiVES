#: src/main.c:2552
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [opts] [filename [start_time] [frames]]\n"
msgstr ""
"\n"
"La syntaxe de lancement est : %s [options] [nomfichier [start_time] "
"[frames]]\n"

#: src/main.c:2553
msgid "Where: filename is the name of a media file or backup file.\n"
msgstr ""
"Où : nomfichier est le nom d'un fichier média ou d'un fichier de "
"sauvegarde.\n"

#: src/main.c:2554
msgid "start_time : filename start time in seconds\n"
msgstr "start_time : temps de début de filename en secondes.\n"

#: src/main.c:2555
msgid "frames : maximum number of frames to open\n"
msgstr "frames : nombre maximal d'images à ouvrir\n"

#: src/main.c:2557
msgid "opts can be:\n"
msgstr "opts peut être :\n"

#: src/main.c:2558
msgid "-help            : show this help text and exit\n"
msgstr "-help : affiche l'aide et quitte.\n"

#: src/main.c:2559
msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr ""
"-tmpdir <repertoiretemporaire> : utiliser un autre répertoire de travail "
"(ex : /var/ramdisk)\n"

#: src/main.c:2560
msgid "-set <setname>   : autoload clip set setname\n"
msgstr ""
"-set <nom_du_set> : charger automatiquement l'ensemble de clips nom_du_set\n"

#: src/main.c:2561
msgid "-noset           : do not load any set on startup\n"
msgstr "-noset : ne charger aucun profil au démarrage\n"

#: src/main.c:2562
msgid "-norecover       : force no-loading of crash recovery\n"
msgstr "-norecover : désactiver la récupération automatique en cas de crash\n"

#: src/main.c:2563
msgid "-recover         : force loading of crash recovery\n"
msgstr ""
"-recover         : force le chargement du recouvrement après un crash\n"

#: src/main.c:2564
msgid ""
"-nothreaddialog  : does nothing - retained for backwards compatibility\n"
msgstr ""

#: src/main.c:2565
msgid "-nogui           : do not show the gui\n"
msgstr "-nogui : lance le programme sans l'interface graphique.\n"

#: src/main.c:2566
msgid "-nosplash        : do not show the splash window\n"
msgstr "-nosplash : ne pas montrer la fenêtre de démarrage\n"

#: src/main.c:2567
msgid "-noplaywin       : do not show the play window\n"
msgstr "-noplaywin : ne pas montrer la fenêtre de lecture\n"

#: src/main.c:2568
msgid "-noninteractive  : disable menu interactivity\n"
msgstr ""

#: src/main.c:2569
msgid "-startup-ce      : start in clip editor mode\n"
msgstr "-startup-ce : démarrer en mode éditeur de clip\n"

#: src/main.c:2570
msgid "-startup-mt      : start in multitrack mode\n"
msgstr "-startup-mt : démarrer en mode multipiste\n"

#: src/main.c:2571
msgid ""
"-fxmodesmax <n>  : allow <n> modes per effect key (minimum is 1, default is "
"8)\n"
msgstr ""
"-fxmodesmax <n> : permet <n> modes par clé d'effet (minimum : 1, valeur par "
"défaut : 8)\n"

#: src/main.c:2573
msgid "-oscstart <port> : start OSC listener on UDP port <port>\n"
msgstr "-oscstart <port> : démarrer l'écoute de OSC sur le port UDP <port>\n"

#: src/main.c:2574
