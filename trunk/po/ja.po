#: src/main.c:2546
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [opts] [filename [start_time] [frames]]\n"
msgstr ""
"\n"
"起動方法: %s [オプション] [ファイル名 [開始時刻] [フレーム]]\n"

#: src/main.c:2547
msgid "Where: filename is the name of a media file or backup file.\n"
msgstr ""
"ファイル名は、メディアファイルもしくはバックアップファイルの名前です。\n"

#: src/main.c:2548
msgid "start_time : filename start time in seconds\n"
msgstr "開始時刻 : ファイル名 開始時刻(秒)\n"

#: src/main.c:2549
msgid "frames : maximum number of frames to open\n"
msgstr "フレーム : オープンする最大フレームの数\n"

#: src/main.c:2551
msgid "opts can be:\n"
msgstr "オプション:\n"

#: src/main.c:2552
msgid "-help            : show this help text and exit\n"
msgstr "-help : ヘルプを表示して終了\n"

#: src/main.c:2553
msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr "-tmpdir <tempdir>: 別な作業用ディレクトリを使う(例 /var/ramdisk)\n"

#: src/main.c:2554
msgid "-set <setname>   : autoload clip set setname\n"
msgstr "-set <setname> : 自動読み込みするクリップをsetnameとする\n"

#: src/main.c:2555
msgid "-noset           : do not load any set on startup\n"
msgstr "-noset : 開始時にセットの読み込みをしない\n"

#: src/main.c:2556
msgid "-norecover       : force no-loading of crash recovery\n"
msgstr "-norecover : 読み込み時にクラッシュからの復元を強制しない\n"

#: src/main.c:2557
msgid "-recover         : force loading of crash recovery\n"
msgstr "-recover : 読み込み時にクラッシュからの復元を強制する\n"

#: src/main.c:2558
#, fuzzy
msgid ""
"-nothreaddialog  : does nothing - retained for backwards compatibility\n"
msgstr "-nothreaddialog  : 何も行いません - 後方互換性のために残しています\n"

#: src/main.c:2559
msgid "-nogui           : do not show the gui\n"
msgstr "-nogui           : GUIを表示しない\n"

#: src/main.c:2560
msgid "-nosplash        : do not show the splash window\n"
msgstr "-nosplash        : スプラッシュウィンドウを表示しない\n"

#: src/main.c:2561
msgid "-noplaywin       : do not show the play window\n"
msgstr "-noplaywin       : プレイウィンドウを表示しない\n"

#: src/main.c:2562
msgid "-noninteractive  : disable menu interactivity\n"
msgstr ""

#: src/main.c:2563
msgid "-startup-ce      : start in clip editor mode\n"
msgstr "-startup-ce      : クリップ編集モードで起動する\n"

#: src/main.c:2564
msgid "-startup-mt      : start in multitrack mode\n"
msgstr "-startup-mt      : マルチトラックモードで起動する\n"

#: src/main.c:2565
msgid ""
"-fxmodesmax <n>  : allow <n> modes per effect key (minimum is 1, default is "
"8)\n"
msgstr ""
"-fxmodesmax <n>  : エフェクトキーそれぞれに <n> つのモードを許可する(最小は"
"1、デフォルトは8)\n"

#: src/main.c:2567
msgid "-oscstart <port> : start OSC listener on UDP port <port>\n"
msgstr "-oscstart <port> : OSCリスナーをUDPポート<port>で開始する\n"

#: src/main.c:2568
