#: src/main.c:1878
#, c-format
msgid "GTK+ version %d.%d.%d (compiled with %d.%d.%d)"
msgstr "Версія GTK+ %d.%d.%d (зібрано з %d.%d.%d)"

#: src/main.c:1890
#, c-format
msgid "GTK+ (compiled with %d.%d.%d)"
msgstr "GTK+ (зібрано з версією %d.%d.%d)"

#: src/main.c:1902
